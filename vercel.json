{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/styles/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.aptidude.in"
        }
      ],
      "destination": "https://aptidude.in/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "https://app.aptidude.in/"
    },
    {
      "source": "/app/question/:path*",
      "destination": "https://app.aptidude.in/question/:path*"
    },
    {
      "source": "/app/learn/:path*",
      "destination": "https://app.aptidude.in/learn/:path*"
    },
    {
      "source": "/question/:path*",
      "destination": "https://app.aptidude.in/question/:path*"
    },
    {
      "source": "/practice",
      "destination": "https://app.aptidude.in/practice"
    },
    {
      "source": "/compete/:path*",
      "destination": "https://app.aptidude.in/compete/:path*"
    },
    {
      "source": "/discuss/:path*",
      "destination": "https://app.aptidude.in/discuss/:path*"
    },
    {
      "source": "/blogs/:path*",
      "destination": "https://app.aptidude.in/blogs/:path*"
    },
    {
      "source": "/profile/:path*",
      "destination": "https://app.aptidude.in/profile/:path*"
    },
    {
      "source": "/admin/:path*",
      "destination": "https://app.aptidude.in/admin/:path*"
    },
    {
      "source": "/login",
      "destination": "https://app.aptidude.in/login"
    },
    {
      "source": "/signup",
      "destination": "https://app.aptidude.in/signup"
    },
    {
      "source": "/analytics",
      "destination": "https://app.aptidude.in/analytics"
    },
    {
      "source": "/about",
