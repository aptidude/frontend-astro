    },
    {
      "source": "/contact",
      "destination": "https://app.aptidude.in/contact"
    },
    {
      "source": "/terms",
      "destination": "https://app.aptidude.in/terms"
    },
    {
      "source": "/privacy",
      "destination": "https://app.aptidude.in/privacy"
    },
    {
      "source": "/cookies",
